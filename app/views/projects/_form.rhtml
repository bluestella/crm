<p>
  <%= f.label :name %><br />
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :desc %><br />
  <%= f.text_area :desc %>
</p>
<p>
  <%= f.label :customer_id %><br />
  <%= f.select :customer_id, @customers, :prompt => "Select..." %>
</p>
<p>
    <b>Members</b><br />
    <% @users.each do |user| %>
        <%= check_box_tag "project[member_ids][]", user.id, @project.members.include?(user) %>
        <%= user.name %>
    <% end %>
</p>